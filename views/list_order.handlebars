<script src="moment.js"></script>
<div class="container">
	<div class="row">	
		<div class="responsive">
			<div class="container col-sm-12 col-lg-12">		
				<table class="brand_list" align="center">	
				<h1 class="text-center" style="margin-bottom:25pt">List of Orders</h1>	
				<tr>
					<th>Name</th>
					<th>Email Address</th>
					<th>Product</th>
					<th>Quantity</th>
					<th>Purchase Date</th>
				</tr>
				{{#each rows}}
				<tr>
					<td>{{this.first_name}} {{this.last_name}}</td>
					<td>{{this.customer_email}}</td>
					<td>{{this.product_name}}</td>
					<td>{{this.quantity}}</td>
					<td><script>moment(this.purchase_date).format('llll');</script></td>			
				</tr>
				{{/each}}
				</table>
			 </div>
		</div>		
	</div>
</div>

